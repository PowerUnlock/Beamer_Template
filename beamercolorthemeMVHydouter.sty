\definecolor{IITHred}{cmy}{0,0.80,1.00}
\definecolor{IITHorange}{cmy}{0,0.60,1.00}
\definecolor{IITHyellow}{cmy}{0,0.40,1.00}

% Changing the palette compositions.
\setbeamercolor*{palette primary}{use=structure,fg=white,bg=IITHred}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=IITHorange}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=IITHorange}
\setbeamercolor*{palette quaternary}{fg=white,bg=IITHorange}


\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!90}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

